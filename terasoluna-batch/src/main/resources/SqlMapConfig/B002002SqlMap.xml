<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="B002002">


	<select id="selectEmployees" resultClass="jp.terasoluna.batch.lite.functionsample.b002.CsvRecord">
		SELECT
            EMPLOYEE_ID AS "id"
            ,EMPLOYEE_FAMILY_NAME AS "familyName"
            ,EMPLOYEE_FIRST_NAME AS "firstName"
            ,EMPLOYEE_AGE AS "age"
		    FROM
		        EMPLOYEE
	</select>

</sqlMap>